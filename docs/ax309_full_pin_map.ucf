##################################################################################
## AX309 Board FPGA Pin Definition 
## FPGA Family: Spartan-6 
## FPGA Model: XC6SLX9
## FPGA Package: FT(G)256
##################################################################################

##################################################################################
## External Clock
##################################################################################
NET "CLK_50M" LOC = T8 | TNM_NET = "sys_clk_pin";
TIMESPEC "TS_sys_clk_pin" = PERIOD "sys_clk_pin" 50 MHz HIGH 50.00%;

##################################################################################
## Custom reset (external pull-up)
##################################################################################
NET "RSTn" LOC = L3 | IOSTANDARD = "LVCMOS33"; ## RESET pushbutton

##################################################################################
## LED Pins 
##################################################################################
NET "led_out<0>"  LOC = P4 | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 8 ; ## LED0
NET "led_out<1>"  LOC = N5 | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 8 ; ## LED1
NET "led_out<2>"  LOC = P5 | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 8 ; ## LED2
NET "led_out<3>"  LOC = M6 | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 8 ; ## LED3

##################################################################################
## KEY Pins (external pull-up)
##################################################################################
NET "key_in<0>"  LOC = C3 | IOSTANDARD = "LVCMOS33" ; ## KEY1
NET "key_in<1>"  LOC = D3 | IOSTANDARD = "LVCMOS33" ; ## KEY2
NET "key_in<2>"  LOC = E4 | IOSTANDARD = "LVCMOS33" ; ## KEY3
NET "key_in<3>"  LOC = E3 | IOSTANDARD = "LVCMOS33" ; ## KEY4

##################################################################################
## Buzzer Pin 
##################################################################################
NET Pin_Out  LOC = J11 | IOSTANDARD = "LVCMOS33";  ##  Buzzer output

##################################################################################
## 7-Segment Display Pins 
##################################################################################
NET SMG_Data<0>  LOC = C7 | IOSTANDARD = "LVCMOS33";    ## DIG0 - segment A 
NET SMG_Data<1>  LOC = E6 | IOSTANDARD = "LVCMOS33";    ## DIG1 - segment B
NET SMG_Data<2>  LOC = C5 | IOSTANDARD = "LVCMOS33";    ## DIG2 - segment C
NET SMG_Data<3>  LOC = F7 | IOSTANDARD = "LVCMOS33";    ## DIG3 - segment D
NET SMG_Data<4>  LOC = D6 | IOSTANDARD = "LVCMOS33";    ## DIG4 - segment E
NET SMG_Data<5>  LOC = E7 | IOSTANDARD = "LVCMOS33";    ## DIG5 - segment F
NET SMG_Data<6>  LOC = D5 | IOSTANDARD = "LVCMOS33";    ## DIG6 - segment G
NET SMG_Data<7>  LOC = C6 | IOSTANDARD = "LVCMOS33";    ## DIG7 - segment H

NET Scan_Sig<5>  LOC = D9 | IOSTANDARD = "LVCMOS33";    ##  SEL0 - digit 1
NET Scan_Sig<4>  LOC = E10 | IOSTANDARD = "LVCMOS33";   ##  SEL1 - digit 2
NET Scan_Sig<3>  LOC = F10 | IOSTANDARD = "LVCMOS33";   ##  SEL2 - digit 3
NET Scan_Sig<2>  LOC = F9 | IOSTANDARD = "LVCMOS33";    ##  SEL3 - digit 4
NET Scan_Sig<1>  LOC = E8 | IOSTANDARD = "LVCMOS33";    ##  SEL4 - digit 5
NET Scan_Sig<0>  LOC = D8 | IOSTANDARD = "LVCMOS33";    ##  SEL5 - digit 6

##################################################################################
## USB-UART Serial Interface Pins 
##################################################################################
NET rx   LOC = C11 | IOSTANDARD = "LVCMOS33";	## Uart RXD:U4_TXD
NET tx   LOC = D12 | IOSTANDARD = "LVCMOS33"; 	## Uart TXD:U4_RXD

##################################################################################
## RTC Clock DS1302 Pins
##################################################################################
NET DS1302_RST   LOC = C13 | IOSTANDARD = "LVCMOS33";  ## 
NET DS1302_SCLK  LOC = E13 | IOSTANDARD = "LVCMOS33";  ## 
NET DS1302_SIO   LOC = D14 | IOSTANDARD = "LVCMOS33";  ## 

##################################################################################
## AT24C04 EEPROM I2C Interface Pins
##################################################################################
NET SCL  LOC = N12 | IOSTANDARD = "LVCMOS33";		  
NET SDA  LOC = P12 | IOSTANDARD = "LVCMOS33"; 

##################################################################################
