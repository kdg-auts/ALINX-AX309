##################################################################################
NET SD_clk      LOC = M3 | IOSTANDARD = "LVCMOS33";   ## CLK/SCK
NET SD_cs       LOC = N3 | IOSTANDARD = "LVCMOS33";   ## DAT3/CS
NET SD_datain   LOC = L5 | IOSTANDARD = "LVCMOS33";   ## CMD/DI
NET SD_dataout  LOC = L4 | IOSTANDARD = "LVCMOS33";   ## DATA0/DO 

